{
  "uid": "cadvisor",
  "title": "cAdvisor Container Metrics",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "Container CPU Usage",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{container_label_com_docker_swarm_service_name=~\".+\"}[1m])",
          "legendFormat": "{{container_label_com_docker_swarm_service_name}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Container Memory Usage",
      "targets": [
        {
          "expr": "container_memory_usage_bytes{container_label_com_docker_swarm_service_name=~\".+\"}",
          "legendFormat": "{{container_label_com_docker_swarm_service_name}}"
        }
      ]
    }
  ],
  "schemaVersion": 36,
  "version": 1
}
